{
  "name": "control-panel",
  "version": "0.0.1",
  "description": "This is Samadhan Tech Task for Anubhav",
  "author": "Anubhav Jain <Anubhavj660@gmail.com>",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "16.16.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "start:all:prod": "pm2 start ecosystem.config.js --env production",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/control-panel/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.1.6",
    "@nestjs/axios": "^1.0.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^10.1.6",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@squareboat/nest-queue-redis": "^0.0.4",
    "@squareboat/nest-queue-strategy": "^0.0.9",
    "apollo-server-express": "^3.11.1",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1197.0",
    "bcrypt": "^5.1.0",
    "bcrypt-nodejs": "^0.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cli-table3": "^0.6.2",
    "ejs": "^3.1.8",
    "graphql": "^16.6.0",
    "helmet": "^5.1.0",
    "html-minifier": "^4.0.0",
    "ioredis": "^5.2.2",
    "json2csv": "^5.0.7",
    "mjml": "^4.13.0",
    "moment": "^2.29.4",
    "nodemailer": "^6.8.0",
    "passport-jwt": "^4.0.0",
    "query-string": "^7.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "uuid": "^9.0.0",
    "webpack": "^5.75.0",
    "yargs-parser": "^21.1.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.8",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.176",
    "@types/mjml": "^4.7.0",
    "@types/node": "^18.0.5",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@types/yargs-parser": "^21.0.0",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "file-loader": "^6.2.0",
    "jest": "28.1.2",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "serverless-webpack": "^5.9.0",
    "source-map": "^0.7.4",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.6",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^4.0.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.4.4",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*.spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@libs/boat(|/.*)$": "<rootDir>/libs/boat/src/$1",
      "^@libs/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@libs/auth(|/.*)$": "<rootDir>/libs/auth/src/$1",
      "^@libs/users(|/.*)$": "<rootDir>/libs/users/src/$1",
      "^@libs/mailman(|/.*)$": "<rootDir>/libs/mailman/src/$1"
    }
  }
}
